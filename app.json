{
  "expo": {
    "name": "Fruits App",
    "slug": "fruits-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/images/icon.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.fruitsapp.mobile",
