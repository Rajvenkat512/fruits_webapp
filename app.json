        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "scheme": "fruitsapp",
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "5fbf92c4-dfb1-4cab-9309-56c40bebabea"
      }
    },
    "owner": "venkatnaidu",
    "plugins": [
      "expo-router"
    ]
  }
}